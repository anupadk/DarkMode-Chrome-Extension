{
	"manifest_version": 2,
	"name": "अँध्यारो मोड",
	"description": "अँध्यारो मोड is the dark themes that is made for protecting eye. ",
	"icons": {"128": "a.png"},
	"version": "0.0.1",

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["website.js"]
		}
	],
